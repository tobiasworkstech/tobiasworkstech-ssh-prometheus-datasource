{
  "name": "tobiasworkstech-sshprometheus-datasource",
  "version": "1.0.1",
  "description": "Prometheus datasource that connects through an SSH tunnel for secure access to remote Prometheus instances",
  "scripts": {
    "build": "webpack -c ./.config/webpack/webpack.config.ts --env production",
    "dev": "webpack -w -c ./.config/webpack/webpack.config.ts --env development",
    "test": "jest --passWithNoTests",
    "test:ci": "jest --passWithNoTests --maxWorkers 4",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --config .config/eslint.config.mjs --cache src",
    "lint:fix": "npm run lint -- --fix",
    "server": "docker compose -f docker/docker-compose.yml up",
    "sign": "npx @grafana/sign-plugin@latest"
  },
  "author": "TobiasWorksTech",
  "license": "Apache-2.0",
  "devDependencies": {
    "@grafana/eslint-config": "^9.0.0",
    "@grafana/tsconfig": "^2.0.1",
    "@swc/core": "^1.15.0",
    "@swc/helpers": "^0.5.0",
    "@swc/jest": "^0.2.0",
    "@testing-library/jest-dom": "^6.6.0",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^29.5.0",
    "@types/lodash": "^4.14.194",
    "@types/node": "^24.0.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "copy-webpack-plugin": "^13.0.0",
    "css-loader": "^7.1.0",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.0",
    "eslint-webpack-plugin": "^5.0.0",
    "fork-ts-checker-webpack-plugin": "^9.1.0",
    "glob": "^11.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.6.0",
    "replace-in-file-webpack-plugin": "^1.0.0",
    "sass": "^1.89.0",
    "sass-loader": "^16.0.0",
    "style-loader": "^4.0.0",
    "swc-loader": "^0.2.0",
    "terser-webpack-plugin": "^5.3.0",
    "ts-node": "^10.9.0",
